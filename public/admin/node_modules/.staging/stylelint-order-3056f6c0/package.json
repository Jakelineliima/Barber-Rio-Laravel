{
  "name": "stylelint-order",
  "version": "4.0.0",
  "description": "A collection of order related linting rules for stylelint.",
  "keywords": [
    "stylelint-plugin",
    "stylelint",
    "css",
    "lint",
    "order"
  ],
  "author": "Aleks Hudochenkov <aleks@hudochenkov.com>",
  "license": "MIT",
  "repository": "hudochenkov/stylelint-order",
  "bugs": {
    "url": "https://github.com/hudochenkov/stylelint-order/issues"
  },
  "homepage": "https://github.com/hudochenkov/stylelint-order",
  "files": [
    "rules",
    "utils",
    "!**/tests",
    "index.js",
    "!.DS_Store"
  ],
  "main": "index.js",
  "dependencies": {
    "lodash": "^4.17.15",
    "postcss": "^7.0.26",
    "postcss-sorting": "^5.0.1"
  },
  "peerDependencies": {
    "stylelint": "^10.0.1 || ^11.0.0 || ^12.0.0 || ^13.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-hudochenkov": "^3.0.1",
    "eslint-config-prettier": "^6.9.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "~1.19.1",
    "stylelint": "^12.0.1"
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "jest": "jest",
    "watch": "jest --watch",
    "coverage": "jest --coverage",
    "fix": "eslint . --fix && prettier '**/*.js' --write"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "./jest-setup.js"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.js$|rules/.*/tests/.*\\.js$"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }

